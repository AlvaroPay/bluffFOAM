FoamFile                                                                           
{                                                                                  
    version     2.0;                                                               
    format      ascii;                                                             
    class       volVectorField;                                                    
    object      U;                                                                 
}                                                                                  
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //    
                                                                                   
dimensions             [0 1 -1 0 0 0 0];                                           
                                                                                   
internalField          uniform (133.9918242212544 0.0 0.0); 
                                                                                   
boundaryField                                                                      
{                                                                                  
   inlet                                                                           
   {                                                                               
       type            flowRateInletVelocity;                                      
       massFlowRate    606.8304733459942; 
       value           $internalField;                                             
   }                                                                               
                                                                                   
   outlet                                                                          
   {                                                                               
       type            zeroGradient;                                               
   }                                                                               
                                                                                   
   wall                                                                            
   {                                                                               
       type            noSlip;                                                     
   }                                                                               
                                                                                   
   "(top|bottom)"                                                                  
   {                                                                               
       type            slip;                                                       
   }                                                                               
                                                                                   
   #includeEtc "caseDicts/setConstraintTypes"                                      
}                                                                                  
                                                                                   
