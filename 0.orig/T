FoamFile                                                                           
{                                                                                  
    version     2.0;                                                               
    format      ascii;                                                             
    class       volScalarField;                                                    
    object      T;                                                                 
}                                                                                  
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //    
                                                                                   
Tinlet         287.3898538366022; 
T0inlet        288.15; 
                                                                                   
dimensions     [0 0 0 1 0 0 0];                                                    
                                                                                   
internalField  uniform $Tinlet;                                                    
                                                                                   
boundaryField                                                                      
{                                                                                  
   inlet                                                                           
   {                                                                               
       type            fixedValue;                                                 
       value           uniform $Tinlet;                                            
   }                                                                               
                                                                                   
   outlet                                                                          
   {                                                                               
       type            totalTemperature;                                           
       T0              uniform $T0inlet;                                           
       gamma           1.4   }                                                                               
                                                                                   
   wall                                                                            
   {                                                                               
       type            zeroGradient;                                               
   }                                                                               
                                                                                   
   #includeEtc "caseDicts/setConstraintTypes"                                      
}                                                                                  
                                                                                   
